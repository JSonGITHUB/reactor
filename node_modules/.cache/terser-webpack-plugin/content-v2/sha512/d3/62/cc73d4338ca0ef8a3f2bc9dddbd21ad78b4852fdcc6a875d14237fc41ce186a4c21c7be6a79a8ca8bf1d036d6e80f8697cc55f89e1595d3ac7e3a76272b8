';\nexport const EDIT_STREAM = 'EDIT_STREAM';\nexport const TIDE = 'TIDE';\nexport const WIND = 'WIND';\nexport const SWELL1_SELECTED = 'SWELL1_SELECTED';\nexport const SWELL2_SELECTED = 'SWELL2_SELECTED';\nexport const SWELL1_DIRECTION = 'SWELL1_DIRECTION';\nexport const SWELL2_DIRECTION = 'SWELL2_DIRECTION';\nexport const SWELL1_ANGLE = 'SWELL1_ANGLE';\nexport const SWELL2_ANGLE = 'SWELL2_ANGLE';\nexport const SWELL1_HEIGHT = 'SWELL1_HEIGHT';\nexport const SWELL2_HEIGHT = 'SWELL2_HEIGHT';\nexport const SWELL1_INTERVAL = 'SWELL1_INTERVAL';\nexport const SWELL2_INTERVAL = 'SWELL2_INTERVAL';\nexport const WIND_DIRECTION = 'WIND_DIRECTION';\nexport const DISTANCE = 'DISTANCE';\nexport const FETCH_GPS = 'FETCH_GPS';\nconst FETCH_POSTS = 'FETCH_POSTS';\n\nexport default FETCH_POSTS;","import React from \"react\";\nimport * as actions from '../actions/types.js';\n\nconst initialTodos = [\n  {\n    id: \"t1\",\n    task: \"Code\",\n    complete: false\n  },\n  {\n    id: \"t2\",\n    task: \"Cook\",\n    complete: false\n  }\n];\nconst todoReducer = (state, action) => {\n    console.log(`todoReducer => `)\n  switch (action.type) {\n    case actions.ADD_TODO:\n        console.log(`ADD_TODO:${JSON.stringify(state,2, null)}`);\n      return initialTodos.map(todo => {\n            console.log(`1b\\ntodo: ${JSON.stringify(todo, 2, null)}`)\n            return todo;\n      });\n    case actions.COMPLETE_TODO:\n        console.log(`COMPLETE_TODO:`)\n      return initialTodos.map(todo => {\n        if (todo.id === action.id) {\n            console.log(`1a\\ntodo.id: ${todo.id}\\naction.id: ${action.id}\\ntodo: ${JSON.stringify(todo, 2, null)}\\ncomplete: ${!todo.complete}`)\n          return { ...todo, complete: true };\n        } else {\n            console.log(`1b\\ntodo: ${JSON.stringify(todo, 2, null)}`)\n          return todo;\n        }\n      });\n    case actions.INCOMPLETE_TODO:\n        console.log(`INCOMPLETE_TODO:`)\n      return initialTodos.map(todo => {\n        if