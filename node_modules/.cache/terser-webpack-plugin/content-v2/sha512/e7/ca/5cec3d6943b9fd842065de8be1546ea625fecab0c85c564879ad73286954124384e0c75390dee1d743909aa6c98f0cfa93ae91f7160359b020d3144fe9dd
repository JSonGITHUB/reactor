ce is great for any repetitve or time regulated events\n//Resize, Scroll inView and Reactive Textfield \nvar debounce=function debounce(func,wait,immediate){var timeout;return function(){var context=this,args=arguments;var later=function later(){timeout=null;if(!immediate)func.apply(context,args);};var callNow=immediate&&!timeout;clearTimeout(timeout);timeout=setTimeout(later,wait);if(callNow)func.apply(context,args);};};//every quarter of a second\n//window.addEventListener('resize', debounce(this.setIt, 250));\n/* harmony default export */ var Debouncer = (debounce);\n// CONCATENATED MODULE: ./src/components/utils/Toggle.js\nvar Toggle_Toggle=function Toggle(props){return/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(\"div\",{className:\"width-100-percent\",children:[/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"div\",{className:\"greet color-red p-5\",children:\"Motion\"}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"button\",{className:\"button-red\",onClick:props.setMotion,children:props.isMotionOn?'ON':'OFF'})]});};/* harmony default export */ var utils_Toggle = (Toggle_Toggle);\n// CONCATENATED MODULE: ./src/components/utils/Greeting.js\nvar Greeting_UserGreeting=function UserGreeting(props){return/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(\"div\",{className:\"greet p-5\",children:[\"Welcome back \",props.user,\"!\"]});};var Greeting_GuestGreeting=function GuestGreeting(){return/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"div\",{className:\"greet p-5\",children:\"Sign up...\"});};var Greeting_Greeting=function Greeting(props){var isLoggedIn=props.isLoggedIn,user=props.user;if(isLoggedIn){return/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(Greeting_UserGreeting,{user:user});}return/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(Greeting_GuestGreeting,{user:user});};/* harmony default export */ var utils_Greeting = (Greeting_Greeting);\n// CONCATENATED MODULE: ./src/components/forms/FormLogin.js\nvar FormLogin_FormLogin=function FormLogin(_ref){var isLoggedIn=_ref.isLoggedIn,use